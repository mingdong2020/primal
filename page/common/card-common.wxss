.ban{
  height:346rpx;
}
.swiper-item{
  display: block;
  height:346rpx; 
}
.wx-swiper-dot.wx-swiper-dot-active{
  padding:0 6rpx;
  border-radius:25px;
}
.ISP-nav{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.ISP-nav:after{
  content: " ";
  position: absolute;
  top: 76rpx;
  height: 1rpx;
  border-bottom: 1rpx solid #E5E5E5;
  color: #E5E5E5;
  left: 30rpx;
  right: 30rpx;
}
.Card-name{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 30rpx;
  font-size: 28rpx;
  color: #858585;
  left: 30rpx;
}
.Card-nav-img{
  margin-right: 30rpx;
  width: 160rpx;
  height: 76rpx;
}
.Card-consultant{
  border-bottom: 18rpx solid #E5E5E5;
}
.Card-title{
  font-size: 28rpx;
  font-weight: 600;
  padding-bottom: 8rpx;
  margin-left: 30rpx;
}
.Card-content{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.Card-items{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
}
.Card-img{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50px;
}
.Panel-Card{
  border-top: 18rpx solid #E5E5E5;
}
.Card-introduce{
  padding: 4rpx 0 4rpx 18rpx;
  font-size: 28rpx;
  color: #616161;
  font-weight: 600;
  line-height: 2.3em;
  border-left: 8rpx solid #1aad19;
  position: relative;
}
.Card-introduce:after{
  content: ' ';
  position: absolute;
  bottom: 0;
  left: 8rpx;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #E5E5E5;
}
.Card-text{
  font-size: 30rpx;
  color: #333333;
  text-indent: 46rpx;
  padding: 16rpx 22rpx 8rpx 22rpx;
}
.Card-box{
  padding: 22rpx 30rpx 16rpx 30rpx;
}
.Card-list{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: top;
  -webkit-align-items: top;
          align-items: top;
}
.Card-sum{
  width: 28rpx;
  height: 28rpx;
}
.Card-list-text{
  -weblit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  min-width: 0;
  font-size: 30rpx;
  color: #333333;
}
.Card-preview{
  padding:0 18rpx 12rpx 0;
  font-size: 28rpx;
  font-weight: 600;
  color: #1aad19;
  text-align: right;
}