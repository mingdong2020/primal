page {
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-family: '微软雅黑', Helvetica, 'Hiragino Sans GB', 'Microsoft Yahei', Arial, sans-serif;
}
.sense{
  height: 100%;
}
.sense_tabbar {
  padding: 0rpx;
}
.sense-search {
  padding-bottom: 10rpx;
}
.sense-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #EFEFF4;
  border-top: 1rpx solid #D7D6DC;
  border-bottom: 1rpx solid #D7D6DC;
}
.sense-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 5px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.sense-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.sense-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}
.sense-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}
.sense-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}
.sense-search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9B9B9B;
  background: #FFFFFF;
  line-height: 28px;
}
.sense-icon-search {
  margin-right: 8px;
  font-size: inherit;
}
.sense-search-bar__text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}
.sense-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09BB07;
  white-space: nowrap;
}
.nav-tabs{
  width: 100%;
  height: 75rpx;
  display: inline-table;   
}
.tab-list{
   width:212rpx;
   margin:20rpx 8rpx 20rpx 20rpx;
   float:left;
}
.tab-image{
  width:48rpx;
  height:48rpx;
  margin:18rpx 0rpx 18rpx 20rpx;
  float:left;
}
.tab-text{
  padding-top:8rpx;
  padding-bottom:4rpx;
  text-align:center;
}
.tab-input{
  width:212rpx;
  margin:20rpx 8rpx 20rpx 20rpx;
  float:left; 
  line-height:75rpx;
  text-align:center;
  flex:1;
  border:1px solid #eee;
  color:#000;
  font-size:28rpx;  
}
.sense-tab{
  width:100%;
  height:65%;
  background-color: #f8f8f8;
}
.tab-content{
  width:100%;
}
.scroll-box{
  height: 100%;
}
 swiper{
  height:100%;
} 
.default{
  line-height:75rpx;
  text-align:center;
  flex:1;
  border:1px solid #eee;
  color:#000;
  font-size:28rpx;
}
.active{
  line-height:75rpx;
  text-align:center;
  color:#fc5558;
  flex:1;
  border:1px solid red;
  font-size:28rpx;
}
.show{
  display:block;
  flex: 1;
}
.hidden{
  display:none;
  flex: 1;
}
.sen-qa{
  padding:12rpx 0rpx;
  margin:10px 0;
  background:#EFEFF4;
  border-top:1rpx solid #D7D6DC;
  border-bottom:1rpx solid #D7D6DC;
}

.sen-question{
   font-size:28rpx;
   color:#212121;
   vertical-align:center;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  min-width: 0;   
}
.sen-answer{
   font-size:28rpx;
   color:#212121;
   vertical-align:center; 
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  min-width: 0;   
}
.sen-block-question{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center; 
}
.sen-block-image{
  width:40rpx;
  height:40rpx;
  text-align:center;
}
.sense-panel {
  position: relative;
  margin-top: 1.17647059em;
  line-height: 1.41176471;
  font-size: 17px;
}
.sense-appmsg {
  margin-top: 0;
}
.sense-appmsg:before {
  display: none;
}
.sense-panel-contain {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.sense-panel-contain:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
  left: 15px;
}
.sense-panel-contain:first-child:before {
  display: none;
}
.sense-panel_access {
  color: inherit;
}
.sense-panel-img{
  width: 48rpx;
  height: 48rpx;
  border-radius:5rpx;
  margin-right: 18rpx;
}
.sense-panel-text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.sense-font{
  font-size:28rpx;
  color:#2e2e2e;
}
.sense-panel_primary {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
}