              <view class="list-img" >
                  <image class="list-image" src="{{list.pic_card}}" />
              </view>
              <view class="list-text">
                  <view class="list-head">名称：{{list.title}}</view>
                  <view class="list-desc">地址：{{list.address}}</view>
                  <view class="list-desc">日期：{{list.setup}}</view>
