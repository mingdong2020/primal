page{
  background-color:#F8F8F8;
  height:100%;
  font-size:32rpx;
  line-height:1.6;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}
.page-section{
  width: 100%;
  height:100%;
}
